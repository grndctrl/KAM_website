<section class="section pt-0 {{ section_class|join(' ') }}">
  <div class="container">

    <div class="row">
      <div class="col hidden md:block md:w-1/12 lg:w-2/12 ">
      </div>

      {% for link in fields.links %}
        

      <div class="col w-5/12 md:w-3/12 lg:w-2/12 pb-1">
          <a target="_blank" href="{{ link.url }}" class="font-label font-medium uppercase anim-underline anim-underline-out">
            {{ link.label }}
          </a>

      </div>

      {% endfor %}
    </div>

  </div>
</section>