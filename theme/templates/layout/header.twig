<header class="header-main header-unpin z-50 hide-top">
  <div class="container show-all">

    {% block header %}

      <nav id="nav-main" class="nav-main flex justify-between items-center" role="navigation">
        <div class="branding z-50 close-menu">
          <a href="{{ site.link }}">
            {% include "svg/kamu-logo.twig" %}
          </a>
        </div>

        <div class="nav-menu font-menu uppercase z-40">
          {% include "components/menu.twig" with {
            "menu" : menu.get_items
          } %}
        </div>

        <div class="nav-overlay font-menu uppercase z-40">
          {% include "components/menu.twig" with {
            "menu" : menu.get_items
          } %}
        </div>

        <div class="nav-burger toggle-menu relative z-50">
          <span></span>
          <span></span>
          <span></span>
        </div>

      </nav>

    {% endblock %}

  </div>
</header>
