@screen md {
  .font-h1 {
    font-size: 112px;
    letter-spacing: -0.4px;
    line-height: 88px;
    text-align: center;
  }
  .font-h2 {
    font-size: 70px;
    letter-spacing: 0em;
    line-height: 72px;
  }
  .font-h3 {
    font-size: 60px;
    letter-spacing: 0.18px;
    line-height: 1em;
  }
  .font-h4 {
    font-size: 40px;
    letter-spacing: 0.18px;
    line-height: 38.4px;
  }
  .font-h5 {
    font-size: 40px;
    letter-spacing: 0.18px;
    line-height: 38.4px;
  }
  .font-menu {
    font-size: 70px;
    /* color: #FDE6DA; */
    letter-spacing: 0;
    line-height: 72px;
  }

  .nav_overlay .fon-menu {
    color: #FDE6DA;
  }
  .font-paragraph {
    font-size: 24px;
    letter-spacing: -0.64px;
    line-height: 34px;
  }
  .font-body {
    font-size: 19.2px;
    letter-spacing: -0.51px;
    line-height: 30.4px;
  }
  .font-label {
    font-size: 16px;
    letter-spacing: -0.43px;
    line-height: 17.6px;
  }
  .font-footer {
    font-size: 20px;
    letter-spacing: -0.025em;
    line-height: 1.5em;
  }
}

@screen lg {
  .font-h1 {
    font-size: 140px;
    letter-spacing: -0.00357em;
    line-height: 0.78571em;
    text-align: center;
  }
  .font-h2 {
    font-size: 110px;
    letter-spacing: -0.00364em;
    line-height: 0.86364em;
  }
  .font-h3 {
    font-size: 75px;
    letter-spacing: -0.00267em;
    line-height: 0.90667em;
  }
  .font-h4 {
    font-size: 50px;
    letter-spacing: 0;
    line-height: 0.96em;
  }
  .font-h5 {
    font-size: 40px;
    letter-spacing: 0;
    line-height: 1em;
  }
  .font-menu {
    font-size: 20px;
    letter-spacing: 0.0555em;
    line-height: 0.725em;
    height: 0.725em;
  }
  .font-paragraph {
    font-size: 30px;
    letter-spacing: -0.02667em;
    line-height: 1.5em;
  }
  .font-body {
    font-size: 24px;
    letter-spacing: -0.025em;
    line-height: 1.58333em;
  }
  .font-label {
    font-size: 20px;
    letter-spacing: -0.025em;
    line-height: 1.1em;
  }
  .font-footer {
    font-size: 20px;
    letter-spacing: -0.025em;
    line-height: 1.5em;
  }
}

@screen fw {
  .font-h1 {
    font-size: calc(min(100vw, $site-max-width) * 140 / 1600);
  }
  .font-h2 {
    font-size: calc(min(100vw, $site-max-width) * 110 / 1600);
  }
  .font-h3 {
    font-size: calc(min(100vw, $site-max-width) * 75 / 1600);
  }
  .font-h4 {
    font-size: calc(min(100vw, $site-max-width) * 50 / 1600);
  }
  .font-h5 {
    font-size: calc(min(100vw, $site-max-width) * 40 / 1600);
  }
  .font-menu {
    font-size: calc(min(100vw, $site-max-width) * 20 / 1600);
  }
  .font-paragraph {
    font-size: calc(min(100vw, $site-max-width) * 30 / 1600);
  }
  .font-body {
    font-size: calc(min(100vw, $site-max-width) * 24 / 1600);
  }
  .font-label {
    font-size: calc(min(100vw, $site-max-width) * 20 / 1600);
  }
}