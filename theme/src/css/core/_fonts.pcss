.font-h1 {
  @apply uppercase overflow-hidden pb-3;
  font-family: GTAmerica-CompressedBlack;
  font-size: 70px;
  letter-spacing: -0.25px;
  line-height: 55px;
}
.font-h2 {
  @apply uppercase overflow-hidden pb-3;
  font-family: GTAmerica-CompressedBlack;
  font-size: 45px;
  letter-spacing: 0;
  line-height: 37.5px;
}
.font-h3 {
  @apply uppercase overflow-hidden pb-3;
  font-family: GTAmerica-CompressedBlack;
  font-size: 40px;
  letter-spacing: 0.2px;
  line-height: 34px;
}
.font-h4 {
  @apply uppercase overflow-hidden pb-3;
  font-family: GTAmerica-CompressedBlack;
  font-size: 35px;
  letter-spacing: 0.2px;
  line-height: 30px;
}
.font-h5 {
  @apply uppercase overflow-hidden pb-3;
  font-family: GTAmerica-CompressedBlack;
  font-size: 40px;
  letter-spacing: 0;
  line-height: 40px;
}
.font-menu {
  @apply uppercase;
  font-family: GTAmerica-CompressedBlack;
  font-size: 45px;
  line-height: 37px;
  letter-spacing: 0;
}
.font-paragraph {
  font-family: GTAmerica-Extended;
  font-size: 24px;
  letter-spacing: -0.6px;
  line-height: 34px;
}
.font-body {
  font-family: GTAmerica-Extended;
  font-size: 18px;
  letter-spacing: -0.5px;
  line-height: 26px;
}
.font-label {
  @apply uppercase font-medium;
  font-family: GTAmerica-Extended;
  font-size: 16px;
  letter-spacing: -0.4px;
  line-height: 17.6px;
  &.font-normal {
    font-weight: 400;
  }
}
.font-small {
  font-family: GTAmerica-Extended;
  font-size: 12px;
  letter-spacing: -0.02667em;
  line-height: 2.5em;
}
.font-footer {
  font-family: GTAmerica-Extended;
  font-size: 16px;
  letter-spacing: -0.4px;  
  p {
    line-height: 1.5em;
  }
}
